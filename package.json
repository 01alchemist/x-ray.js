{
  "name": "x-ray.js",
  "version": "0.0.1",
  "description": "Global Illumination using parallel path tracing",
  "main": "index.js",
  "scripts": {
    "test": "echo \"Error: no test specified\" && exit 1",
    "build": "tsc -p ./modules/x-ray.js",
    "build-example": "tsc -p ./example",
    "build-tst": "tsc ./modules/x-ray.js/test/TextureTest.ts",
    "build-worker": "tsc -p ./modules/x-ray.js/workers",
    "build-amd-worker": "tsc -p ./modules/x-ray.js/workers -m amd",
    "bundle": "tsc -p ./modules/x-ray.js -d --outFile ./modules/x-ray.js/bundle/system/x-ray.js.js",
    "bundle-amd": "tsc -p ./modules/x-ray.js -m amd -d --outFile ./modules/x-ray.js/bundle/amd/x-ray.js.js"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/01alchemist/ParallelRayJS.git"
  },
  "author": "",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/01alchemist/ParallelRayJS/issues"
  },
  "homepage": "https://github.com/01alchemist/ParallelRayJS",
  "dependencies": {
    "systemjs": "0.19.6",
    "xdata": "^2.1.15"
  },
  "devDependencies": {
    "turbo.js": "^1.0.0",
    "typescript": "^1.8.10"
  }
}
