{
  "name": "xrenderer",
  "version": "0.0.1",
  "description": "Global Illumination using parallel path tracing",
  "main": "index.js",
  "scripts": {
    "test": "echo \"Error: no test specified\" && exit 1",
    "build": "tsc -p ./modules/xrenderer",
    "build-example": "tsc -p ./example",
    "build-tst": "tsc ./modules/xrenderer/test/TextureTest.ts",
    "build-worker": "tsc -p ./modules/xrenderer/workers",
    "build-amd-worker": "tsc -p ./modules/xrenderer/workers -m amd",
    "bundle": "tsc -p ./modules/xrenderer -d --outFile ./modules/xrenderer/bundle/system/xrenderer.js",
    "bundle-amd": "tsc -p ./modules/xrenderer -m amd -d --outFile ./modules/xrenderer/bundle/amd/xrenderer.js"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/01alchemist/ParallelRayJS.git"
  },
  "author": "",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/01alchemist/ParallelRayJS/issues"
  },
  "homepage": "https://github.com/01alchemist/ParallelRayJS",
  "dependencies": {
    "systemjs": "0.19.6",
    "xdata": "^2.1.15"
  },
  "devDependencies": {
    "typescript": "^1.8.10"
  }
}
